{
  "name": "slack-irc",
  "version": "3.6.1",
  "description": "Connects IRC and Slack channels by sending messages back and forth. Sajt version with userlist command.",
  "keywords": [
    "slack",
    "irc",
    "gateway",
    "bot",
    "slack-irc"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:sajt/slack-irc.git"
  },
  "bugs": {
    "url": "https://github.com/sajt/slack-irc/issues"
  },
  "scripts": {
    "start": "node index.js",
    "lint": "eslint . --ignore-path .gitignore",
    "coverage": "istanbul cover _mocha $(find test -name '*.test.js') && istanbul report cobertura",
    "test": "npm run lint && npm run coverage"
  },
  "bin": {
    "slack-irc": "./index.js"
  },
  "author": {
    "name": "Martin Ek <mail@ekmartin.no>"
  },
  "license": "MIT",
  "dependencies": {
    "check-env": "~1.2.0",
    "commander": "~2.8.1",
    "irc": "~0.3.9",
    "lodash": "~3.9.3",
    "slack-client": "~1.4.0",
    "winston": "~1.0.0",
    "vow": "~0.4.11"
  },
  "devDependencies": {
    "chai": "~2.3.0",
    "eslint": "~1.6.0",
    "eslint-config-airbnb": "~0.1.0",
    "istanbul": "~0.3.5",
    "jscs": "~1.13.1",
    "jshint": "~2.7.0",
    "mocha": "~2.2.5",
    "rewire": "~2.3.1",
    "sinon": "~1.14.1",
    "sinon-chai": "~2.7.0"
  }
}
